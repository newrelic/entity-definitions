{
	"name": "Aruba AP",
	"description": null,
	"pages": [
	  {
		"name": "Aruba AP",
		"description": null,
		"widgets": [
		  {
			"title": "Summary",
			"layout": {
			  "column": 1,
			  "row": 1,
			  "width": 3,
			  "height": 5
			},
			"visualization": {
			  "id": "viz.billboard"
			},
			"rawConfiguration": {
			  "dataFormatters": [
				{
				  "name": "Last Update",
				  "type": "date"
				},
				{
				  "name": "Uptime (Days)",
				  "precision": 2,
				  "type": "decimal"
				}
			  ],
			  "nrqlQueries": [
				{
				  "accountId": 0,
				  "query": "FROM Metric SELECT latest(device_name) AS 'Device Name', latest(src_addr) AS 'Device IP', latest(kentik.snmp.Uptime)/8640000 AS 'Uptime (Days)', latest(SysObjectID) AS 'SysObjectID', latest(entity.type) AS 'NR Entity Type', latest(instrumentation.name) AS 'Ktranslate Profile', latest(timestamp) AS 'Last Update' WHERE provider = 'kentik-wap'"
				}
			  ],
			  "thresholds": []
			}
		  },
		  {
			"title": "Current CPU %",
			"layout": {
			  "column": 4,
			  "row": 1,
			  "width": 4,
			  "height": 1
			},
			"visualization": {
			  "id": "viz.billboard"
			},
			"rawConfiguration": {
			  "dataFormatters": [],
			  "nrqlQueries": [
				{
				  "accountId": 0,
				  "query": "FROM Metric SELECT latest(kentik.snmp.CPU) AS 'Current CPU %' WHERE provider = 'kentik-wap'"
				}
			  ],
			  "thresholds": [
				{
				  "alertSeverity": "WARNING",
				  "value": 90
				},
				{
				  "alertSeverity": "CRITICAL",
				  "value": 95
				}
			  ]
			}
		  },
		  {
			"title": "Current Memory %",
			"layout": {
			  "column": 8,
			  "row": 1,
			  "width": 4,
			  "height": 1
			},
			"visualization": {
			  "id": "viz.billboard"
			},
			"rawConfiguration": {
			  "dataFormatters": [],
			  "nrqlQueries": [
				{
				  "accountId": 0,
				  "query": "FROM Metric SELECT latest(kentik.snmp.MemoryUtilization) AS 'Current Memory %' WHERE provider = 'kentik-wap'"
				}
			  ],
			  "thresholds": [
				{
				  "alertSeverity": "WARNING",
				  "value": 90
				},
				{
				  "alertSeverity": "CRITICAL",
				  "value": 95
				}
			  ]
			}
		  },
		  {
			"title": "CPU Utilization Timeline",
			"layout": {
			  "column": 4,
			  "row": 2,
			  "width": 4,
			  "height": 4
			},
			"visualization": {
			  "id": "viz.line"
			},
			"rawConfiguration": {
			  "legend": {
				"enabled": true
			  },
			  "nrqlQueries": [
				{
				  "accountId": 0,
				  "query": "FROM Metric SELECT average(kentik.snmp.CPU) AS 'Average', min(kentik.snmp.CPU) AS 'Min', max(kentik.snmp.CPU) AS 'Max' WHERE provider = 'kentik-wap' TIMESERIES 5 MINUTES"
				}
			  ],
			  "yAxisLeft": {
				"max": 100,
				"min": 0,
				"zero": false
			  }
			}
		  },
		  {
			"title": "Memory Utilization Timeline",
			"layout": {
			  "column": 8,
			  "row": 2,
			  "width": 4,
			  "height": 4
			},
			"visualization": {
			  "id": "viz.line"
			},
			"rawConfiguration": {
			  "legend": {
				"enabled": true
			  },
			  "nrqlQueries": [
				{
				  "accountId": 0,
				  "query": "FROM Metric SELECT average(kentik.snmp.MemoryUtilization) AS 'Average', min(kentik.snmp.MemoryUtilization) AS 'Min', max(kentik.snmp.MemoryUtilization) AS 'Max' WHERE provider = 'kentik-wap' TIMESERIES 5 MINUTES"
				}
			  ],
			  "yAxisLeft": {
				"max": 100,
				"min": 0,
				"zero": false
			  }
			}
		  },
		  {
			"title": "Interface Status",
			"layout": {
			  "column": 1,
			  "row": 9,
			  "width": 3,
			  "height": 4
			},
			"visualization": {
			  "id": "viz.table"
			},
			"rawConfiguration": {
			  "dataFormatters": [],
			  "facet": {
				"showOtherSeries": false
			  },
			  "nrqlQueries": [
				{
				  "accountId": 0,
				  "query": "FROM Metric SELECT latest(if_OperStatus) AS 'Operational Status' FACET if_interface_name AS 'Interface', if_Type AS 'Interface Type' WHERE provider = 'kentik-wap' LIMIT MAX"
				}
			  ]
			}
		  },
		  {
			"title": "Receive Traffic Mbps",
			"layout": {
			  "column": 4,
			  "row": 9,
			  "width": 4,
			  "height": 4
			},
			"visualization": {
			  "id": "viz.area"
			},
			"rawConfiguration": {
			  "facet": {
				"showOtherSeries": false
			  },
			  "legend": {
				"enabled": true
			  },
			  "nrqlQueries": [
				{
				  "accountId": 0,
				  "query": "FROM Metric SELECT rate(max(kentik.snmp.ifHCInOctets)*8/1000/1000, 1 second) AS 'Mbps' FACET if_interface_name WHERE provider = 'kentik-wap' TIMESERIES 5 MINUTES LIMIT MAX"
				}
			  ]
			}
		  },
		  {
			"title": "Transmit Traffic Mbps",
			"layout": {
			  "column": 8,
			  "row": 9,
			  "width": 4,
			  "height": 4
			},
			"visualization": {
			  "id": "viz.area"
			},
			"rawConfiguration": {
			  "facet": {
				"showOtherSeries": false
			  },
			  "legend": {
				"enabled": true
			  },
			  "nrqlQueries": [
				{
				  "accountId": 0,
				  "query": "FROM Metric SELECT rate(max(kentik.snmp.ifHCOutOctets)*8/1000/1000, 1 second) AS 'Mbps' FACET if_interface_name WHERE provider = 'kentik-wap' TIMESERIES 5 MINUTES LIMIT MAX"
				}
			  ]
			}
		  }
		]
	  }
	]
  }
