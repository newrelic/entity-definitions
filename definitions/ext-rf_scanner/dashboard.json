              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "from THD_RF_TRANSACTION select  latest(serialNumber),latest(ipAddress),latest(lastActiveTimestamp) since 12 hours ago"
              }
            ]
          }
