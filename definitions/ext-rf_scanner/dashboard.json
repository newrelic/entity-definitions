            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "select average(totalTime) from THD_RF_TRANSACTION TIMESERIES since 12 hours ago"
              }
            ],
            "yAxisLeft": {
              "zero": true
            }
          }
        },
        {
          "visualization": {
