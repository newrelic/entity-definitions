          {
            "title": "CPU Utilization (%)",
            "layout": {
              "column": 3,
              "row": 2,
              "width": 4,
              "height": 4
            },
            "visualization": {
              "id": "viz.line"
            },
            "rawConfiguration": {
              "facet": {
                "showOtherSeries": false
              },
              "legend": {
                "enabled": true
