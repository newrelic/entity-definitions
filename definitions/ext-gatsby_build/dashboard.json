            "facet": {
              "showOtherSeries": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "SELECT average(`duration.ms`)/1000 FROM Span FACET api LIMIT 1000"
              }
            ]
          }
        }
