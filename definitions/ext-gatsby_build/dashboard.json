          "title": "Latest Build Times (in s)",
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Span SELECT latest(duration.ms)/1000 FACET buildId "
