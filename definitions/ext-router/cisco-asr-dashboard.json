{
  "name": "Cisco ASR Router",
  "description": null,
  "pages": [
    {
      "name": "Cisco ASR Router",
      "description": null,
      "widgets": [
        {
          "visualization": {
            "id": "viz.billboard"
          },
          "layout": {
            "column": 1,
            "row": 1,
            "height": 4,
            "width": 4
          },
          "title": "Summary",
          "rawConfiguration": {
            "dataFormatters": [],
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Metric select latest(device_name) AS 'Device Name',latest(src_addr) as 'Device IP', latest(SysObjectID) AS 'SysObjectID', latest(entity.type) AS 'Entity Type', latest(instrumentation.name) AS 'Ktranslate Profile', latest(kentik.snmp.Uptime)/8640000 as 'Uptime (Days)', latest(timestamp) as 'Last Update', latest(PollingHealth) where provider = 'kentik-router'"
              }
            ],
            "thresholds": []
          }
        },
        {
          "visualization": {
            "id": "viz.billboard"
          },
          "layout": {
            "column": 5,
            "row": 1,
            "height": 1,
            "width": 4
          },
          "title": "Current CPU",
          "rawConfiguration": {
            "dataFormatters": [],
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Metric SELECT max(kentik.snmp.CPU) AS 'CPU Utilization %' WHERE provider = 'kentik-router'"
              }
            ],
            "thresholds": [
              {
                "alertSeverity": "WARNING",
                "value": 90
              },
              {
                "alertSeverity": "CRITICAL",
                "value": 95
              }
            ]
          }
        },
        {
          "visualization": {
            "id": "viz.billboard"
          },
          "layout": {
            "column": 9,
            "row": 1,
            "height": 1,
            "width": 4
          },
          "title": "Current Memory",
          "rawConfiguration": {
            "dataFormatters": [],
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Metric SELECT max(kentik.snmp.MemoryUtilization) AS 'Memory Utilization %' WHERE provider = 'kentik-router'"
              }
            ],
            "thresholds": [
              {
                "alertSeverity": "WARNING",
                "value": 90
              },
              {
                "alertSeverity": "CRITICAL",
                "value": 95
              }
            ]
          }
        },
        {
          "visualization": {
            "id": "viz.line"
          },
          "layout": {
            "column": 5,
            "row": 2,
            "height": 3,
            "width": 4
          },
          "title": "CPU Utilization (%)",
          "rawConfiguration": {
            "dataFormatters": [],
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Metric SELECT average(kentik.snmp.CPU) FACET entity.name WHERE provider = 'kentik-router' TIMESERIES 5 minutes"
              }
            ],
            "yAxisLeft": {
              "max": 100,
              "min": 0,
              "zero": false
            }
          }
        },
        {
          "visualization": {
            "id": "viz.line"
          },
          "layout": {
            "column": 9,
            "row": 2,
            "height": 3,
            "width": 4
          },
          "title": "Memory Utilization (%)",
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Metric SELECT average(kentik.snmp.MemoryUtilization) FACET entity.name WHERE provider = 'kentik-router' TIMESERIES 5 MINUTES"
              }
            ],
            "yAxisLeft": {
              "max": 100,
              "min": 0,
              "zero": false
            }
          }
        },
        {
          "visualization": {
            "id": "viz.table"
          },
          "layout": {
            "column": 1,
            "row": 5,
            "height": 3,
            "width": 12
          },
          "title": "Interfaces Summary",
          "rawConfiguration": {
            "dataFormatters": [],
            "facet": {
              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Metric SELECT latest(kentik.snmp.IfInUtilization) AS 'RX %', latest(kentik.snmp.IfOutUtilization) AS 'TX %', latest(ifOperStatus) AS 'Operational Status', sum(kentik.snmp.ifInErrors) AS 'RX Errors', sum(kentik.snmp.ifOutErrors) AS 'TX Errors' FACET if_name or if_interface_name AS 'Interface', if_Alias AS 'Interface Alias', if_Speed WHERE provider = 'kentik-router' LIMIT MAX"
              }
            ]
          }
        },
        {
          "visualization": {
            "id": "viz.line"
          },
          "layout": {
            "column": 1,
            "row": 8,
            "height": 3,
            "width": 6
          },
          "title": "Top Receive Traffic %",
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Metric SELECT average(kentik.snmp.IfInUtilization) as 'RX %' FACET if_interface_name WHERE provider = 'kentik-router' TIMESERIES 5 MINUTES limit 10"
              }
            ],
            "yAxisLeft": {
              "max": 100,
              "min": 0,
              "zero": false
            }
          }
        },
        {
          "visualization": {
            "id": "viz.line"
          },
          "layout": {
            "column": 7,
            "row": 8,
            "height": 3,
            "width": 6
          },
          "title": "Top Transmit Traffic %",
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Metric SELECT average(kentik.snmp.IfOutUtilization) as 'TX %' FACET if_interface_name WHERE provider = 'kentik-router' TIMESERIES 5 MINUTES limit 10"
              }
            ],
            "yAxisLeft": {
              "max": 100,
              "min": 0,
              "zero": false
