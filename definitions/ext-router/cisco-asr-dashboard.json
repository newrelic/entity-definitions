            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Metric SELECT rate(max(kentik.snmp.ifHCInOctets)*8/1000/1000, 1 second) AS 'Mbps' FACET if_interface_name WHERE provider = 'kentik-router' TIMESERIES 5 MINUTES limit 10"
              }
            ],
            "yAxisLeft": {
              "zero": false
            }
          }
        },
        {
          "visualization": {
            "id": "viz.line"
          },
