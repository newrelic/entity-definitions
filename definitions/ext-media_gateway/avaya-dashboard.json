    "pages": [
      {
        "name": "Avaya Media Gateway",
        "description": null,
        "widgets": [
          {
            "visualization": {
              "id": "viz.billboard"
            },
            "layout": {
              "column": 1,
              "row": 1,
              "height": 6,
              "width": 2
            },
            "title": "Summary",
            "rawConfiguration": {
              "dataFormatters": [
                {
                  "name": "Last Update",
                  "type": "date"
                },
                {
                  "name": "Uptime (Days)",
                  "precision": 2,
                  "type": "decimal"
                }
              ],
              "nrqlQueries": [
                {
                  "accountId": 0,
                  "query": "FROM Metric SELECT latest(device_name) AS 'Device Name', latest(src_addr) AS 'Device IP', latest(kentik.snmp.Uptime)/8640000 AS 'Uptime (Days)', latest(SysObjectID) AS 'SysObjectID', latest(entity.type) AS 'Entity Type', latest(instrumentation.name) AS 'Ktranslate Profile', latest(timestamp) AS 'Last Update', latest(PollingHealth) AS 'Polling Health', latest(PollingHealthReason) AS 'Health Reason' WHERE provider = 'kentik-avaya-gateway'"
                }
              ],
              "thresholds": []
            }
          },
          {
            "visualization": {
              "id": "viz.billboard"
            },
            "layout": {
              "column": 3,
              "row": 1,
              "height": 3,
              "width": 2
            },
            "title": "Registration State",
            "rawConfiguration": {
              "dataFormatters": [],
              "nrqlQueries": [
                {
                  "accountId": 0,
                  "query": "FROM Metric SELECT latest(kentik.snmp.cmgRegistrationState) FACET cmgRegistrationState WHERE provider = 'kentik-avaya-gateway'"
                }
              ],
              "thresholds": [
                {
                  "alertSeverity": "CRITICAL",
                  "value": 2
                }
              ]
            }
          },
          {
            "visualization": {
              "id": "viz.billboard"
            },
