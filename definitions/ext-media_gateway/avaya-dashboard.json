                }
              ],
              "thresholds": [
                {
                  "alertSeverity": "WARNING",
                  "value": 90
                },
                {
                  "alertSeverity": "CRITICAL",
                  "value": 95
                }
              ]
            }
          },
          {
            "visualization": {
              "id": "viz.billboard"
            },
