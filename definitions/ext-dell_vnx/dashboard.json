          "layout": {
            "column": 1,
            "row": 1,
            "height": 3,
            "width": 4
          },
          "title": "Pool Used %",
          "rawConfiguration": {
            "dataFormatters": [],
            "facet": {
              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM vnx_capacity SELECT latest(poolSizeUsed/poolSizeTotal)*100 as 'Pool Used %',latest(poolSizeFree)/1024/1024/1024 as 'Free GB',latest(poolSizeTotal)/1024/1024/1024 as 'Total GB',latest(poolSizeUsed)/1024/1024/1024 as 'Used GB' where key = 'pool_capacity' facet poolName "
              }
            ]
          }
        },
        {
          "visualization": {
            "id": "viz.line"
          },
          "layout": {
            "column": 5,
            "row": 1,
            "height": 3,
            "width": 4
          },
          "title": "Cluster Capacity Used %",
          "rawConfiguration": {
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM vnx_capacity SELECT latest((sizeUsed / sizeTotal))*100 as 'Used %'  WHERE key = 'cluster_capacity' TIMESERIES 5 minutes "
              }
            ],
            "yAxisLeft": {
              "zero": true
            }
          }
        },
