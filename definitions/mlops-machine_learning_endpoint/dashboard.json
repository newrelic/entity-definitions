          "visualization": {
            "id": "viz.line"
          },
          "layout": {
            "column": 1,
            "row": 1,
            "height": 3,
            "width": 4
          },
          "title": "CPU Utilization (Percent)",
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Metric SELECT average(`aws..aws.sagemaker.endpoints.CPUUtilization`) where `aws..aws.sagemaker.endpoints.CPUUtilization` is NOT NULL TIMESERIES 1 minute UNTIL 3 minutes ago LIMIT 1024 "
              }
            ],
            "yAxisLeft": {
              "zero": true
            }
          }
        },
