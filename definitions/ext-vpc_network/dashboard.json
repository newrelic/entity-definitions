{
  "name": "VPC Network",
  "description": null,
  "pages": [
    {
      "name": "VPC Network",
      "description": null,
      "widgets": [
        {
          "visualization": {
            "id": "viz.billboard"
          },
          "layout": {
            "column": 1,
            "row": 1,
            "height": 3,
            "width": 3
          },
          "title": "",
          "rawConfiguration": {
            "dataFormatters": [],
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Metric SELECT latest(application_type) AS 'VPC Source', latest(vpc_region) AS 'Region'"
              }
            ],
            "thresholds": []
          }
        },
        {
          "visualization": {
            "id": "viz.line"
          },
          "layout": {
            "column": 4,
            "row": 1,
            "height": 3,
            "width": 3
          },
          "title": "Total Endpoints",
          "rawConfiguration": {
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Metric SELECT uniqueCount(ip_address) FACET entity.name TIMESERIES"
              }
            ],
            "yAxisLeft": {
              "zero": false
            }
          }
        },
        {
          "visualization": {
            "id": "viz.line"
          },
          "layout": {
            "column": 7,
            "row": 1,
            "height": 3,
            "width": 3
          },
          "title": "Total Conversations (Endpoint:Port)",
          "rawConfiguration": {
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Metric SELECT uniqueCount(tuple(ip_address,port)) FACET entity.name WHERE metricName LIKE 'kentik.rollup.vpc.%.bytes' TIMESERIES "
              }
            ],
            "yAxisLeft": {
              "zero": false
            }
          }
        },
        {
          "visualization": {
            "id": "viz.bar"
          },
          "layout": {
            "column": 10,
            "row": 1,
            "height": 3,
            "width": 3
          },
          "title": "Top 5 - MBps Per Geo",
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Metric SELECT (rate(sum(kentik.rollup.vpc.xmt.bytes)*8/1000/1000, 1 second) + rate(sum(kentik.rollup.vpc.rcv.bytes)*8/1000/1000, 1 second)) FACET country LIMIT 5"
              }
            ]
          }
        },
        {
          "visualization": {
            "id": "viz.line"
          },
          "layout": {
            "column": 1,
            "row": 4,
            "height": 4,
            "width": 6
          },
          "title": "Transmit Mbps",
          "rawConfiguration": {
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Metric SELECT rate(sum(kentik.rollup.vpc.xmt.bytes)*8/1000/1000, 1 second) FACET entity.name TIMESERIES"
              }
            ],
            "yAxisLeft": {
              "zero": false
            }
          }
        },
        {
          "visualization": {
            "id": "viz.line"
          },
          "layout": {
            "column": 7,
            "row": 4,
            "height": 4,
            "width": 6
          },
          "title": "Receive Mbps",
          "rawConfiguration": {
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Metric SELECT rate(sum(kentik.rollup.vpc.rcv.bytes)*8/1000/1000, 1 second) FACET entity.name TIMESERIES"
              }
            ],
            "yAxisLeft": {
              "zero": false
            }
          }
        }
      ]
    }
  ]
}
