          "visualization": {
            "id": "viz.markdown"
          },
          "rawConfiguration": {
            "text": "# AWS Backup\nAWS Backup is a fully-managed service that makes it easy to centralize and automate data protection across AWS services, in the cloud, and on premises. Using this service, you can configure backup policies and monitor activity for your AWS resources in one place."
          }
        },
        {
          "title": "NumberOfBackupJobsCreated",
          "layout": {
            "column": 3,
            "row": 1,
            "width": 10,
            "height": 2
          },
          "visualization": {
            "id": "viz.line"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Metric SELECT sum(aws.backup.NumberOfBackupJobsCreated) AS 'NumberOfBackupJobsCreated' TIMESERIES AUTO FACET aws.backup.ResourceType"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            },
            "yAxisLeft": {
              "zero": true
            },
            "yAxisRight": {
              "zero": true
            }
