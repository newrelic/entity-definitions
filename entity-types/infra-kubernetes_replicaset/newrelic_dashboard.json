            "nrqlQueries": [
              {
                "query": "from InfrastructureEvent select count(concat(event.type, ':', event.reason)) as 'Reason (Type)' where clusterName = '{{{clusterName}}}' and event.metadata.namespace = '{{{namespaceName}}}' and category = 'kubernetes' and (event.involvedObject.name = '{{{entity.name}}}' OR event.involvedObject.name like '%{{{entity.name}}}%') and event.type = 'Warning' facet event.reason TIMESERIES limit 100",
                "accountId": 0}
            ],
            "facet": {
              "showOtherSeries": false
            }
          }
        }
      ]
    }
  ]
}
