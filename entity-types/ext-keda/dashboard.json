{
  "name": "KEDA Metrics Dashboard",
  "description": "A dashboard for monitoring KEDA metrics from Prometheus.",
  "pages":
    [
      {
        "name": "KEDA Overview",
        "description": null,
        "widgets":
          [
            {
              "title": "",
              "layout": { "column": 1, "row": 1, "width": 2, "height": 3 },
              "visualization": { "id": "viz.markdown" },
              "rawConfiguration":
                {
                  "text": "![KEDA Logo](https://raw.githubusercontent.com/kedacore/keda/refs/heads/main/images/logos/keda-icon-colour.svg)\n\n# KEDA Dashboard\n\nMetrics for the KEDA (Kubernetes-based Event Driven Autoscaling) operator."
                }
            },
            {
              "title": "Registered Resources",
              "layout": { "column": 3, "row": 1, "width": 5, "height": 3 },
              "visualization": { "id": "viz.line" },
              "rawConfiguration":
                {
                  "nrqlQueries":
                    [
                      {
                        "accountId": 0,
                        "query": "FROM Metric SELECT sum(keda_resource_registered_total) FACET type, exported_namespace TIMESERIES"
                      }
                    ],
                  "yAxisLeft": { "zero": true }
                }
            },
            {
              "title": "Registered Triggers",
              "layout": { "column": 8, "row": 1, "width": 5, "height": 3 },
              "visualization": { "id": "viz.line" },
              "rawConfiguration":
                {
                  "nrqlQueries":
                    [
                      {
                        "accountId": 0,
                        "query": "FROM Metric SELECT sum(keda_trigger_registered_total) FACET type TIMESERIES"
                      }
                    ],
                  "yAxisLeft": { "zero": true }
                }
            },
            {
              "title": "Scaled Object Errors Rate",
              "layout": { "column": 1, "row": 4, "width": 6, "height": 4 },
              "visualization": { "id": "viz.line" },
              "rawConfiguration":
                {
                  "nrqlQueries":
                    [
                      {
                        "accountId": 0,
                        "query": "FROM Metric SELECT rate(sum(keda_scaled_object_errors_total), 2 minute) FACET scaledObject, exported_namespace TIMESERIES"
                      }
                    ],
                  "yAxisLeft": { "zero": true }
                }
            },
            {
              "title": "Scaler Errors Rate",
              "layout": { "column": 7, "row": 4, "width": 6, "height": 4 },
              "visualization": { "id": "viz.line" },
              "rawConfiguration":
                {
                  "nrqlQueries":
                    [
                      {
                        "accountId": 0,
                        "query": "FROM Metric SELECT rate(sum(keda_scaler_detail_errors_total), 2 minute) FACET scaledObject, exported_namespace, type, scaler TIMESERIES"
                      }
                    ],
                  "yAxisLeft": { "zero": true }
                }
            },
            {
              "title": "Scaler Metrics Value",
              "layout": { "column": 1, "row": 8, "width": 6, "height": 4 },
              "visualization": { "id": "viz.line" },
              "rawConfiguration":
                {
                  "nrqlQueries":
                    [
                      {
                        "accountId": 0,
                        "query": "FROM Metric SELECT latest(keda_scaler_metrics_value) FACET scaledObject, exported_namespace, type, scaler TIMESERIES"
                      }
                    ],
                  "yAxisLeft": { "zero": true }
                }
            },
            {
              "title": "Scaler Metrics Latency",
              "layout": { "column": 7, "row": 8, "width": 6, "height": 4 },
              "visualization": { "id": "viz.line" },
              "rawConfiguration":
                {
                  "nrqlQueries":
                    [
                      {
                        "accountId": 0,
                        "query": "FROM Metric SELECT latest(keda_scaler_metrics_latency_seconds) FACET scaledObject, exported_namespace, type, scaler TIMESERIES"
                      }
                    ],
                  "yAxisLeft": { "zero": true }
                }
            },
            {
              "title": "Active Scalers",
              "layout": { "column": 1, "row": 12, "width": 6, "height": 4 },
              "visualization": { "id": "viz.line" },
              "rawConfiguration":
                {
                  "nrqlQueries":
                    [
                      {
                        "accountId": 0,
                        "query": "FROM Metric SELECT latest(keda_scaler_active) WHERE keda_scaler_active = 1 AND job = '{{tags.job}}' FACET scaledObject, exported_namespace, type, scaler TIMESERIES"
                      }
                    ],
                  "yAxisLeft": { "zero": true }
                }
            },
            {
              "title": "gRPC Server Handled Rate",
              "layout": { "column": 7, "row": 12, "width": 6, "height": 4 },
              "visualization": { "id": "viz.line" },
              "rawConfiguration":
                {
                  "nrqlQueries":
                    [
                      {
                        "accountId": 0,
                        "query": "FROM Metric SELECT rate(sum(keda_internal_metricsservice_grpc_server_handled_total), 2 minute) FACET grpc_code TIMESERIES"
                      }
                    ],
                  "yAxisLeft": { "zero": true }
                }
            },
            {
              "title": "gRPC Server Handling Duration (p50/p95/p99)",
              "layout": { "column": 1, "row": 16, "width": 6, "height": 4 },
              "visualization": { "id": "viz.line" },
              "rawConfiguration":
                {
                  "nrqlQueries":
                    [
                      {
                        "accountId": 0,
                        "query": "FROM Metric SELECT percentile(keda_internal_metricsservice_grpc_server_handling_seconds, 99, 95, 50) FACET grpc_service, grpc_method TIMESERIES"
                      }
                    ],
                  "yAxisLeft": { "zero": true }
                }
            },
            {
              "title": "Scale Loop Latency",
              "layout": { "column": 7, "row": 16, "width": 6, "height": 4 },
              "visualization": { "id": "viz.line" },
              "rawConfiguration":
                {
                  "nrqlQueries":
                    [
                      {
                        "accountId": 0,
                        "query": "FROM Metric SELECT rate(sum(keda_internal_scale_loop_latency_seconds), 2 minute) FACET resource, exported_namespace, type TIMESERIES"
                      }
                    ],
                  "yAxisLeft": { "zero": true }
                }
            }
          ]
      }
    ]
}