                  "yAxisLeft": { "zero": true }
                }
            },
            {
              "title": "gRPC Server Handling Duration (p50/p95/p99)",
              "layout": { "column": 7, "row": 32, "width": 6, "height": 4 },
              "visualization": { "id": "viz.line" },
              "rawConfiguration":
                {
                  "nrqlQueries":
                    [
                      {
                        "accountId": 0,
