{
    "name": "Servertech PDU",
    "description": null,
    "pages": [
      {
        "name": "Servertech PDU",
        "description": null,
        "widgets": [
          {
            "title": "Summary",
            "layout": {
              "column": 1,
              "row": 1,
              "width": 3,
              "height": 6
            },
            "visualization": {
              "id": "viz.billboard"
            },
            "rawConfiguration": {
              "dataFormatters": [
                {
                  "name": "Last Update",
                  "type": "recent-relative"
                }
              ],
              "facet": {
                "showOtherSeries": false
              },
              "nrqlQueries": [
                {
                  "accountId": 0,
                  "query": "FROM Metric SELECT\n  latest(device_name) AS 'Device Name',\n  latest(src_addr) AS 'Device IP',\n  latest(kentik.snmp.Uptime)/8640000 AS 'Uptime (Days)',\n  latest(SysLocation) AS 'Location',\n  latest(SysObjectID) AS 'SysObjectID',\n  latest(entity.type) AS 'NR Entity Type',\n  latest(instrumentation.name) AS 'Ktranslate Profile',\n  latest(timestamp) AS 'Last Update',\n  latest(tags.container_service) AS 'Docker Container',\n  latest(PollingHealth) AS 'Polling Health',\n  latest(PollingHealthReason) AS 'Health Reason'\nWHERE provider = 'kentik-pdu'\nLIMIT MAX"
                }
              ],
              "platformOptions": {
                "ignoreTimeRange": false
              }
            }
          },
          {
            "title": "Unit Details",
            "layout": {
              "column": 4,
              "row": 1,
              "width": 3,
              "height": 6
            },
            "visualization": {
              "id": "viz.billboard"
            },
            "rawConfiguration": {
              "facet": {
                "showOtherSeries": false
              },
              "nrqlQueries": [
                {
                  "accountId": 0,
                  "query": "FROM Metric SELECT\n  latest(st4UnitName) AS 'Unit Name',\n  latest(entity_serial) AS 'Unit Serial',\n  latest(st4UnitModel) AS 'Unit Model',\n  latest(st4UnitType) AS 'Unit Type',\n  latest(st4SystemFirmwareVersion) AS 'Unit Firmware',\n  latest(st4UnitProductMfrDate) AS 'Unit Manufacturer Date'\nWHERE provider = 'kentik-pdu'\nLIMIT MAX"
                }
              ],
              "platformOptions": {
                "ignoreTimeRange": false
              }
            }
          },
          {
            "title": "Unit Status",
