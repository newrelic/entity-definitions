{
    "name": "OracleDb",
    "pages": [
        {
            "name": "OracleDbSampleOpenTelemetry",
            "widgets": [
                {
                    "visualization": {
                        "id": "viz.line"
                    },
                    "layout": {
                        "column": 1,
                        "row": 1,
                        "height": 3,
                        "width": 3
                    },
                    "title": "CPU Time",
                    "rawConfiguration": {
                        "nrqlQueries": [
                            {
                                "query": "SELECT latest(oracledb.cpu_time) FROM Metric TIMESERIES AUTO ",
                                "accountId": 0}
                        ]
                    }
                },
                {
                    "visualization": {
                        "id": "viz.line"
                    },
                    "layout": {
                        "column": 1,
                        "row": 1,
                        "height": 3,
                        "width": 3
                    },
                    "title": "Sessions Usage",
                    "rawConfiguration": {
                        "nrqlQueries": [
                            {
                                "query": "SELECT latest(oracledb.sessions.usage) FROM Metric TIMESERIES AUTO ",
                                "accountId": 0}
                        ]
                    }
                },
                {
                    "visualization": {
                        "id": "viz.line"
                    },
                    "layout": {
                        "column": 1,
                        "row": 1,
                        "height": 3,
                        "width": 3
                    },
                    "title": "Total Executions",
                    "rawConfiguration": {
                        "nrqlQueries": [
                            {
                                "query": "SELECT latest(oracledb.executions) FROM Metric TIMESERIES AUTO",
                                "accountId": 0}
                        ]
                    }
                }
            ]
        }
    ]
}