            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "FROM Metric select average(aws.timestream.SuccessfulRequestLatency ) as 'Successful Request Latency (ms)'"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
