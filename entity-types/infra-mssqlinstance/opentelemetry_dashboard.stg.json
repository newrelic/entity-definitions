          "rawConfiguration": {
            "nrqlQueries": [
              {
                "query": "SELECT rate(sum(sqlserver.page.checkpoint.flush.rate), 1 minute) AS 'Checkpoint pages' FROM Metric TIMESERIES AUTO",
                "accountId": 0}
            ]
          }
        },
        {
          "visualization": {
            "id": "viz.line"
          },
          "layout": {
            "column": 5,
