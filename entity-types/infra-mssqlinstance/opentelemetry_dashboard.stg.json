{
  "name": "SQLServerSampleOpenTelemetry",
  "pages": [
    {
      "name": "SQLServerSampleOpenTelemetry",
      "widgets": [
        {
          "visualization": {
            "id": "viz.area"
          },
          "layout": {
            "column": 1,
            "row": 1,
            "height": 3,
            "width": 4
          },
          "title": "Connections",
          "rawConfiguration": {
            "nrqlQueries": [
              {
                "query": "SELECT average(sqlserver.user.connection.count) AS 'Connections' FROM Metric TIMESERIES AUTO",
                "accountId": 0}
            ]
          }
        },
        {
          "visualization": {
            "id": "viz.area"
          },
          "layout": {
            "column": 5,
            "row": 1,
            "height": 3,
            "width": 4
          },
          "title": "SQL compilations and re-compilations",
          "rawConfiguration": {
            "nrqlQueries": [
              {
                "query": "SELECT rate(sum(sqlserver.batch.sql_compilation.rate), 1 minute) AS 'Compilations', rate(sum(sqlserver.batch.sql_recompilation.rate), 1 minute) AS 'Re-compilations' FROM Metric TIMESERIES AUTO",
                "accountId": 0}
            ]
          }
        },
        {
          "visualization": {
            "id": "viz.area"
          },
          "layout": {
            "column": 9,
            "row": 1,
            "height": 3,
            "width": 4
          },
          "title": "Buffer cache hit percentage",
          "rawConfiguration": {
            "nrqlQueries": [
              {
                "query": "SELECT average(sqlserver.page.buffer_cache.hit_ratio) AS 'Cache hit' FROM Metric TIMESERIES AUTO",
                "accountId": 0}
            ]
          }
        },
        {
          "visualization": {
            "id": "viz.area"
          },
          "layout": {
            "column": 1,
            "row": 4,
            "height": 3,
            "width": 4
          },
          "title": "Average batch requests",
          "rawConfiguration": {
            "nrqlQueries": [
              {
                "query": "SELECT rate(sum(sqlserver.batch.request.rate), 1 minute) AS 'Batch requests' FROM Metric TIMESERIES AUTO",
                "accountId": 0}
            ]
          }
        },
        {
          "visualization": {
            "id": "viz.line"
          },
          "layout": {
            "column": 5,
            "row": 4,
            "height": 3,
            "width": 4
          },
          "title": "Page life expectancy",
          "rawConfiguration": {
            "nrqlQueries": [
              {
                "query": "SELECT average(sqlserver.page.life_expectancy) * 1000 AS 'Page life expentancy' FROM Metric TIMESERIES AUTO",
                "accountId": 0}
            ]
          }
        },
        {
          "visualization": {
            "id": "viz.area"
          },
          "layout": {
            "column": 9,
            "row": 4,
            "height": 3,
            "width": 4
          },
          "title": "Number of blocked processes",
