{
  "name": "MyStore Monitoring",
  "description": null,
  "pages": [
    {
      "name": "MyStore Monitoring",
      "description": null,
      "widgets": [
        {
          "title": "Free Space (MB)",
          "layout": {
            "column": 1,
            "row": 1,
            "width": 6,
            "height": 3
          },
          "visualization": {
            "id": "viz.area"
          },
          "rawConfiguration": {
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "SELECT average(FreeSpaceMB) FROM MyStore_FileSize_SQL WHERE FileName = 'SDM' AND store.number = '{{tags.store.number}}' TIMESERIES AUTO"
              }
            ]
          }
        },
        {
          "title": "Current Size (MB)",
          "layout": {
            "column": 7,
            "row": 1,
            "width": 6,
            "height": 3
          },
          "visualization": {
            "id": "viz.area"
          },
          "rawConfiguration": {
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "SELECT average(CurrentSizeMB) FROM MyStore_FileSize_SQL WHERE FileName = 'SDM' AND store.number = '{{tags.store.number}}' TIMESERIES AUTO"
              }
            ]
          }
        }
      ]
    }
  ]
}
