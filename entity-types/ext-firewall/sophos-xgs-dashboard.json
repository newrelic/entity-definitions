          {
            "title": "Summary",
            "layout": {
              "column": 1,
              "row": 1,
              "width": 4,
              "height": 5
            },
            "visualization": {
              "id": "viz.billboard"
            },
            "rawConfiguration": {
              "dataFormatters": [
                {
                  "name": "Last Update",
                  "type": "date"
                },
                {
                  "name": "Uptime (Days)",
                  "precision": 2,
                  "type": "decimal"
                }
              ],
              "facet": {
                "showOtherSeries": false
              },
              "nrqlQueries": [
                {
                  "accountId": 0,
                  "query": "FROM Metric SELECT latest(device_name) AS 'Device Name', latest(src_addr) AS 'Device IP', latest(kentik.snmp.Uptime)/8640000 AS 'Uptime (Days)', latest(sfosDeviceType) AS 'Device Type', latest(sfosDeviceFWVersion) AS 'Firmware Version', latest(sfosIPSVersion) AS 'Snort Version', latest(SysObjectID) AS 'SysObjectID', latest(entity.type) AS 'Entity Type', latest(instrumentation.name) AS 'Ktranslate Profile', latest(timestamp) AS 'Last Update', latest(PollingHealth) AS 'Polling Health', latest(PollingHealthReason) AS 'Health Reason' WHERE provider = 'kentik-firewall'"
                }
              ],
              "platformOptions": {
                "ignoreTimeRange": false
              }
            }
          },
