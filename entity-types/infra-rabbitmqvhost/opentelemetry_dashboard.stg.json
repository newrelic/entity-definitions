{
  "name": "RabbitMQ VHost Metrics (OpenTelemetry)",
  "pages": [
    {
      "name": "RabbitMQ VHost Metrics",
      "widgets": [
        {
          "visualization": {
            "id": "viz.line"
          },
          "layout": {
            "column": 1,
            "row": 1,
            "height": 3,
            "width": 4
          },
          "title": "Messages Published",
          "rawConfiguration": {
            "nrqlQueries": [
              {
                "query": "FROM Metric SELECT sum(rabbitmq.message.published) WHERE instrumentation.provider = 'opentelemetry' TIMESERIES AUTO",
                "accountId": 0
              }
            ],
            "yAxisLeft": {
              "zero": true
            }
          }
        },
        {
          "visualization": {
            "id": "viz.line"
          },
          "layout": {
            "column": 5,
            "row": 1,
            "height": 3,
            "width": 4
          },
          "title": "Messages Delivered",
          "rawConfiguration": {
            "nrqlQueries": [
              {
                "query": "FROM Metric SELECT sum(rabbitmq.message.delivered) WHERE instrumentation.provider = 'opentelemetry' TIMESERIES AUTO",
                "accountId": 0
              }
            ],
            "yAxisLeft": {
              "zero": true
            }
          }
        },
        {
          "visualization": {
            "id": "viz.line"
          },
          "layout": {
            "column": 9,
            "row": 1,
            "height": 3,
            "width": 4
          },
          "title": "Messages Acknowledged",
          "rawConfiguration": {
            "nrqlQueries": [
              {
                "query": "FROM Metric SELECT sum(rabbitmq.message.acknowledged) WHERE instrumentation.provider = 'opentelemetry' TIMESERIES AUTO",
                "accountId": 0
              }
            ],
            "yAxisLeft": {
              "zero": true
            }
          }
        },
        {
          "visualization": {
            "id": "viz.line"
          },
          "layout": {
            "column": 1,
            "row": 4,
            "height": 3,
            "width": 4
          },
          "title": "Current Messages by State",
          "rawConfiguration": {
            "nrqlQueries": [
              {
                "query": "FROM Metric SELECT sum(rabbitmq.message.current) WHERE instrumentation.provider = 'opentelemetry' FACET state TIMESERIES AUTO",
                "accountId": 0
              }
            ],
            "yAxisLeft": {
              "zero": true
            }
          }
        },
        {
          "visualization": {
            "id": "viz.line"
          },
          "layout": {
            "column": 5,
            "row": 4,
            "height": 3,
            "width": 4
          },
          "title": "Consumer Count",
          "rawConfiguration": {
            "nrqlQueries": [
              {
                "query": "FROM Metric SELECT sum(rabbitmq.consumer.count) WHERE instrumentation.provider = 'opentelemetry' TIMESERIES AUTO",
                "accountId": 0
              }
            ],
            "yAxisLeft": {
              "zero": true
            }
          }
        },
        {
          "visualization": {
            "id": "viz.billboard"
          },
          "layout": {
            "column": 9,
            "row": 4,
            "height": 3,
            "width": 4
          },
          "title": "VHost Health Summary",
          "rawConfiguration": {
            "nrqlQueries": [
              {
                "query": "FROM Metric SELECT sum(rabbitmq.message.published) AS 'Messages Published', sum(rabbitmq.message.current) AS 'Current Messages', sum(rabbitmq.consumer.count) AS 'Active Consumers', uniqueCount(rabbitmq.queue.name) AS 'Total Queues' WHERE instrumentation.provider = 'opentelemetry' AND metricName IN ('rabbitmq.message.published', 'rabbitmq.message.current', 'rabbitmq.consumer.count')",
                "accountId": 0
              }
            ],
            "thresholds": []
          }
        },
        {
          "visualization": {
            "id": "viz.line"
          },
          "layout": {
            "column": 1,
            "row": 7,
            "height": 3,
            "width": 6
          },
          "title": "Message Flow Overview",
          "rawConfiguration": {
            "nrqlQueries": [
              {
                "query": "FROM Metric SELECT sum(rabbitmq.message.published) AS 'Published', sum(rabbitmq.message.delivered) AS 'Delivered', sum(rabbitmq.message.acknowledged) AS 'Acknowledged' WHERE instrumentation.provider = 'opentelemetry' TIMESERIES AUTO",
                "accountId": 0
              }
            ],
            "yAxisLeft": {
              "zero": true
            }
          }
        },
        {
          "visualization": {
            "id": "viz.table"
          },
          "layout": {
            "column": 7,
            "row": 7,
            "height": 3,
            "width": 6
          },
          "title": "VHost Queue Details",
          "rawConfiguration": {
            "nrqlQueries": [
              {
                "query": "FROM Metric SELECT sum(rabbitmq.message.current) AS 'Current Messages', sum(rabbitmq.consumer.count) AS 'Consumers', sum(rabbitmq.message.published) AS 'Published', sum(rabbitmq.message.delivered) AS 'Delivered' WHERE instrumentation.provider = 'opentelemetry' AND rabbitmq.queue.name IS NOT NULL FACET rabbitmq.queue.name",
                "accountId": 0
              }
            ]
          }
        },
        {
          "visualization": {
            "id": "viz.line"
          },
          "layout": {
            "column": 1,
            "row": 10,
            "height": 3,
            "width": 6
          },
          "title": "Message Throughput Rate",
          "rawConfiguration": {
            "nrqlQueries": [
              {
                "query": "FROM Metric SELECT rate(sum(rabbitmq.message.published), 1 second) AS 'Published/sec', rate(sum(rabbitmq.message.delivered), 1 second) AS 'Delivered/sec', rate(sum(rabbitmq.message.acknowledged), 1 second) AS 'Acknowledged/sec' WHERE instrumentation.provider = 'opentelemetry' TIMESERIES AUTO",
                "accountId": 0
              }
            ],
            "yAxisLeft": {
              "zero": true
            }
          }
        },
        {
          "visualization": {
            "id": "viz.pie"
          },
          "layout": {
            "column": 7,
            "row": 10,
            "height": 3,
            "width": 6
          },
          "title": "Message Distribution by Queue",
          "rawConfiguration": {
            "nrqlQueries": [
              {
                "query": "FROM Metric SELECT sum(rabbitmq.message.current) WHERE instrumentation.provider = 'opentelemetry' AND rabbitmq.queue.name IS NOT NULL FACET rabbitmq.queue.name",
                "accountId": 0
              }
            ]
          }
        }
      ]
    }
  ]
}