            "initialSorting": {
              "direction": "desc",
              "name": "Avg Exec Time (ms)"
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "SELECT latest(db.namespace) AS `Database`,\n       latest(db.query.text) AS `Query Text`,\n       latest(postgresql.rolname) AS `Role`,\n       latest(postgresql.calls) AS `Execution Count`,\n       latest(postgresql.total_exec_time) AS `Avg Exec Time (ms)`,\n       latest(postgresql.shared_blks_read) AS `Avg Disk Reads`,\n       latest(postgresql.shared_blks_written) AS `Avg Disk Writes`\nFROM Log\nFACET postgresql.queryid limit MAX\n"
