              "direction": "desc",
              "name": "averageExecutionTimems"
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "SELECT\n  latest(postgresql.total_exec_time) / latest(postgresql.calls) as averageExecutionTimems\nFROM Log\nWHERE postgresql.total_exec_time IS NOT NULL\nfacet db.query.text\n"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
