			"title": "Flow Device Summary",
			"rawConfiguration": {
			  "dataFormatters": [],
			  "nrqlQueries": [
				{
				  "accountId": 0,
				  "query": "FROM KFlow SELECT latest(device_name), latest(Type or type) AS 'Flow Type', rate(count(*),1 second) AS 'Flows per Second', latest(timestamp) AS 'Last Update', uniqueCount(src_addr, dst_addr) AS 'Observed IP Addresses' "
				}
			  ],
